<module>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.i18n.I18N'/>
    <inherits name='com.totsp.gwittir.Emul'/>
    <inherits name='com.totsp.gwittir.Binding' />
    <inherits name='com.totsp.gwittir.FX' />



    <replace-with
            class="com.totsp.gwittir.mvc.ui.rebind.FlashElementFactoryEmbed">
        <when-type-is
                class="com.totsp.gwittir.mvc.ui.rebind.FlashElementFactory" />
        <when-property-is name="user.agent" value="gecko"/>
    </replace-with>
    <replace-with
            class="com.totsp.gwittir.mvc.ui.rebind.FlashElementFactoryEmbed">
        <when-type-is
                class="com.totsp.gwittir.mvc.ui.rebind.FlashElementFactory" />
        <when-property-is name="user.agent" value="gecko1_8"/>
    </replace-with>
    <replace-with
            class="com.totsp.gwittir.mvc.ui.rebind.FlashElementFactoryEmbed">
        <when-type-is
                class="com.totsp.gwittir.mvc.ui.rebind.FlashElementFactory" />
        <when-property-is name="user.agent" value="ie6"/>
    </replace-with>
</module>