
<module>
    <inherits name="com.google.gwt.core.Core" />
    <inherits name="com.google.gwt.logging.Logging" />
    <source path="introspection" />
    <generate-with
            class="com.totsp.gwittir.rebind.introspection.IntrospectorGenerator" >
        <when-type-assignable
                class="com.totsp.gwittir.introspection.Introspector" />
    </generate-with>

</module>