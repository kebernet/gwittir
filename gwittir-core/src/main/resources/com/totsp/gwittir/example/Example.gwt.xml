<?xml version="1.0" encoding="UTF-8"?>
<module>
    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name="com.google.gwt.i18n.I18N"/>
    <!-- Inherit the java.beans.* classes from GWTx           -->
    <inherits name='com.googlecode.gwtx.Java'/>
    <inherits name='com.totsp.gwittir.Gwittir' />
    <inherits name='com.totsp.gwittir.Logging' />
    <inherits name='com.totsp.gwittir.skins.basic.Basic' />
    <inherits name='org.goda.Time' />
    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.totsp.gwittir.example.client.ExampleEntryPoint'/>
    <stylesheet src="example.css"/>
    <servlet path="/ExampleStreamService"
     class="com.totsp.gwittir.example.server.ExampleStreamServiceServlet" />
     <servlet path="/ReadSourceService"
     class="com.totsp.gwittir.example.server.ReadSourceServiceServlet" />
</module>