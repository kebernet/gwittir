<?xml version="1.0" encoding="UTF-8"?>

<module>
	<inherits name='com.totsp.gwittir.Gwittir' />
    <inherits name='com.google.gwt.gears.Gears'/>
    <replace-with
        class="com.totsp.gwittir.gears.client.util.impl.WindowContextPersisterGears">
        <when-type-is
            class="com.totsp.gwittir.client.util.impl.WindowContextPersister" />
        <when-property-is name="gears.installed" value="true"/>
    </replace-with>
</module>