<module>
    <inherit name="com.google.gwt.user.User" />
    <source path="fx" />
    <replace-with
            class="com.totsp.gwittir.fx.rebind.OpacitySetterIE6">
        <when-type-is
                class="com.totsp.gwittir.fx.rebind.OpacitySetter" />
        <when-property-is name="user.agent" value="ie6"/>
    </replace-with>
    <replace-with
            class="com.totsp.gwittir.fx.rebind.ReflectionIE6">
        <when-type-is
                class="com.totsp.gwittir.fx.rebind.Reflection" />
        <when-property-is name="user.agent" value="ie6"/>
    </replace-with>
    <replace-with
            class="com.totsp.gwittir.fx.rebind.DimensionsIE6">
        <when-type-is
                class="com.totsp.gwittir.fx.rebind.Dimensions" />
        <when-property-is name="user.agent" value="ie6"/>
    </replace-with>
</module>