<?xml version="1.0" encoding="UTF-8"?>
<module>
    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    
    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name="com.google.gwt.i18n.I18N"/>
    
    <!-- Inherit the java.beans.* classes from GWTx           -->    
    <inherits name='com.googlecode.gwtx.Java'/>
    
    <!--  Inherit Gwittir stuff -->
    <inherits name='com.totsp.gwittir.Gwittir' />
    <inherits name="com.totsp.gwittir.LoggingDisabled"/> 
    
    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.totsp.gwittir.example.client.ContactsEntryPoint'/>
    
    <!--  Include Gwittir styling -->
    <inherits name='com.totsp.gwittir.skins.basic.Basic' />
    <!-- ><stylesheet src="contacts.css"/> -->
    
    <servlet path="/ContactsRemoteService" class="com.totsp.gwittir.example.remote.ContactsServiceServlet" />
</module>